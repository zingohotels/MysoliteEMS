<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card-view="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/tools"
    tools:context="app.zingo.mysolite.ui.NewAdminDesigns.AdminDashBoardFragment">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/scroll_view"
        android:scrollbars="none">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <View
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:id="@+id/linear_title"
                android:background="@color/colorPrimary" />

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:id="@+id/cardview_data_count"
                android:layout_height="100dp"
                android:layout_marginTop="-30dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_below="@+id/linear_title"
                card-view:cardElevation="5dp"
                card_view:cardCornerRadius="20dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/dept_count_layout"
                        android:orientation="vertical">

                        <app.zingo.mysolite.Custom.MyRegulerText
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="12sp"
                            android:gravity="center_horizontal"
                            android:text="Department"
                            android:paddingLeft="10dp"
                            android:paddingTop="10dp"
                            android:textColor="#000"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center_horizontal"
                            android:orientation="horizontal">
                            <app.zingo.mysolite.Custom.MyRegulerText
                                android:id="@+id/dept_count_text"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:textSize="20sp"
                                android:gravity="center"
                                android:text="0"
                                android:paddingTop="5dp"
                                android:paddingLeft="15dp"
                                android:textColor="#AFB42B" />

                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/employee_count_layout"
                        android:orientation="vertical">

                        <app.zingo.mysolite.Custom.MyRegulerText
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="12sp"
                            android:gravity="center_horizontal"
                            android:text="Employee"
                            android:paddingLeft="10dp"
                            android:paddingTop="10dp"
                            android:textColor="#000" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center_horizontal"
                            android:orientation="horizontal">

                            <app.zingo.mysolite.Custom.MyRegulerText
                                android:id="@+id/employee_count_text"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:paddingLeft="15dp"
                                android:paddingTop="5dp"
                                android:text="0"
                                android:textColor="#FBC02D"
                                android:textSize="20sp" />


                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/on_task_count_layout"
                        android:orientation="vertical">

                        <app.zingo.mysolite.Custom.MyRegulerText
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="12sp"
                            android:gravity="center_horizontal"
                            android:text="On Task"
                            android:paddingLeft="10dp"
                            android:paddingTop="10dp"
                            android:textColor="#000"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center_horizontal"
                            android:orientation="horizontal">
                            <app.zingo.mysolite.Custom.MyRegulerText
                                android:id="@+id/on_task_count_text"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:textSize="20sp"
                                android:text="0"
                                android:paddingTop="5dp"
                                android:paddingLeft="15dp"
                                android:textColor="#D81B60"/>

                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/pending_task_layout"
                        android:orientation="vertical">

                        <app.zingo.mysolite.Custom.MyRegulerText
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="12sp"
                            android:gravity="center_horizontal"
                            android:text="Pending"
                            android:paddingLeft="10dp"
                            android:paddingTop="10dp"
                            android:textColor="#000"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center_horizontal"
                            android:orientation="horizontal">
                            <app.zingo.mysolite.Custom.MyRegulerText
                                android:id="@+id/pending_task_text"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:textSize="20sp"
                                android:text="0"
                                android:paddingTop="5dp"
                                android:paddingLeft="15dp"
                                android:textColor="#F44336" />

                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <app.zingo.mysolite.Custom.MyRegulerText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="14sp"
                android:id="@+id/atten_title"
                android:layout_below="@+id/cardview_data_count"
                android:gravity="left"
                android:text="Today Attendance"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                android:textColor="#000"
                />



            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:id="@+id/cardview_data_count_employee"
                android:layout_below="@+id/atten_title"
                android:layout_height="100dp"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="5dp"
                card-view:cardElevation="5dp"
                card_view:cardCornerRadius="20dp">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/present_count_layout"
                        android:orientation="vertical">

                        <app.zingo.mysolite.Custom.MyRegulerText
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="12sp"
                            android:gravity="center_horizontal"
                            android:text="Present"
                            android:paddingLeft="10dp"
                            android:paddingTop="10dp"
                            android:textColor="#000"
                            />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center_horizontal"
                            android:orientation="horizontal">
                            <app.zingo.mysolite.Custom.MyRegulerText
                                android:id="@+id/today_employee_present"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:textSize="20sp"
                                android:gravity="center"
                                android:text="0"
                                android:paddingTop="5dp"
                                android:paddingLeft="15dp"
                                android:textColor="#4CAF50"
                                />

                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/absent_count_layout"
                        android:orientation="vertical">

                        <app.zingo.mysolite.Custom.MyRegulerText
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="12sp"
                            android:gravity="center_horizontal"
                            android:text="Absent"
                            android:paddingLeft="10dp"
                            android:paddingTop="10dp"
                            android:textColor="#000"
                            />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center_horizontal"
                            android:orientation="horizontal">
                            <app.zingo.mysolite.Custom.MyRegulerText
                                android:id="@+id/absent_employee"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:textSize="20sp"
                                android:text="0"
                                android:paddingTop="5dp"
                                android:paddingLeft="15dp"
                                android:textColor="#0288D1"
                                />


                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:id="@+id/leave_count_layout"
                        android:orientation="vertical">

                        <app.zingo.mysolite.Custom.MyRegulerText
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="12sp"
                            android:gravity="center_horizontal"
                            android:text="Leave"
                            android:paddingLeft="10dp"
                            android:paddingTop="10dp"
                            android:textColor="#000"
                            />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center_horizontal"
                            android:orientation="horizontal">
                            <app.zingo.mysolite.Custom.MyRegulerText
                                android:id="@+id/leave_employees"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:textSize="20sp"
                                android:text="0"
                                android:paddingTop="5dp"
                                android:paddingLeft="15dp"
                                android:textColor="#720D5D"
                                />


                        </LinearLayout>
                    </LinearLayout>


                </LinearLayout>
            </androidx.cardview.widget.CardView>


