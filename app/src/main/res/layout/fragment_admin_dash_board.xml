            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/cardview_data_count_employee"
                android:id="@+id/live_lays"
                android:orientation="vertical">

                <app.zingo.mysolite.Custom.MyRegulerText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:gravity="left"
                    android:text="Employees Locations"
                    android:paddingLeft="10dp"
                    android:paddingTop="10dp"
                    android:textColor="#000"
                    />

                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:id="@+id/noLocationFound"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="40dp"

                    android:layout_below="@+id/linearLayout2">
                    <ImageView
                        android:id="@+id/imageView3"
                        android:background="@color/white"
                        android:layout_width="48dp"
                        android:layout_height="48dp"
                        android:src="@drawable/search"
                        android:minHeight="100dp"
                        android:alpha="0.7"
                        android:contentDescription="TODO" />
                    <TextView
                        android:textSize="15sp"
                        android:textColor="@color/text_grey"
                        android:gravity="center_horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="No Location Found"/>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/location_list_track"
                    android:visibility="gone"
                    android:clickable="true"
                    android:focusable="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"/>

                <app.zingo.mysolite.Custom.MyRegulerText
                    android:id="@+id/read_more_live"
                    android:layout_width="120dp"
                    android:layout_height="0dp"
                    android:layout_gravity="right"
                    android:layout_margin="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/button_primary_dark"
                    android:focusable="false"
                    android:gravity="center"

                    android:text="View More"
                    android:textAlignment="center"
                    android:textColor="@color/white"
                    android:textSize="14dp"
                    android:visibility="gone"
                    />



                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.3dp"
                    android:background="#99A1AC"
                    android:layout_marginTop="2dp"/>




            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/live_lays"

                android:layout_alignParentStart="true"
                android:layout_marginTop="-1dp"
                android:orientation="vertical">

                <app.zingo.mysolite.Custom.MyRegulerText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/cardview_data_count"
                    android:gravity="left"
                    android:paddingLeft="10dp"
                    android:paddingTop="10dp"
                    android:text="Today Tasks"
                    android:textColor="#000"
                    android:textSize="14sp" />

                <LinearLayout
                    android:id="@+id/noRecordFound"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/linearLayout2"
                    android:layout_marginTop="40dp"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageView2"
                        android:layout_width="48dp"
                        android:layout_height="48dp"
                        android:alpha="0.7"
                        android:background="@color/white"
                        android:minHeight="100dp"
                        android:src="@drawable/search"
                        android:contentDescription="TODO" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:text="No Task Found"
                        android:textColor="@color/text_grey"
                        android:textSize="15sp" />
                </LinearLayout>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/task_list_dash"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:clickable="true"
                    android:focusable="true"/>

                <app.zingo.mysolite.Custom.MyRegulerText
                    android:id="@+id/read_more"
                    android:layout_width="120dp"
                    android:layout_height="0dp"
                    android:layout_gravity="right"
                    android:layout_margin="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/button_primary_dark"
                    android:focusable="false"
                    android:gravity="center"

                    android:text="View More"
                    android:textAlignment="center"
                    android:textColor="@color/white"
                    android:textSize="14dp"
                    android:visibility="gone" />


                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.3dp"
                    android:layout_marginTop="2dp"
                    android:background="#99A1AC" />

                <app.zingo.mysolite.Custom.MyRegulerText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:layout_below="@+id/cardview_data_count"
                    android:gravity="left"
                    android:text="Meetings"
                    android:paddingLeft="10dp"
                    android:paddingTop="10dp"
                    android:textColor="#000" />

                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:id="@+id/noRecordFound_meetings"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="40dp"

                    android:layout_below="@+id/linearLayout2">
                    <ImageView
                        android:id="@+id/imageView2_meeting"
                        android:background="@color/white"
                        android:layout_width="48dp"
                        android:layout_height="48dp"
                        android:src="@drawable/search"
                        android:minHeight="100dp"
                        android:alpha="0.7"
                        android:contentDescription="TODO" />
                    <TextView
                        android:textSize="15sp"
                        android:textColor="@color/text_grey"
                        android:gravity="center_horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="No Meetings Found"/>
                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/targetList_meeting"
                    android:visibility="gone"
                    android:focusable="false"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"/>

                <app.zingo.mysolite.Custom.MyRegulerText
                    android:id="@+id/read_meeting"
                    android:layout_weight="1"
                    android:layout_width="150dp"
                    android:visibility="gone"
                    android:layout_height="45dp"
                    android:background="@drawable/button_primary_dark"
                    android:layout_margin="5dp"
                    android:textAlignment="center"
