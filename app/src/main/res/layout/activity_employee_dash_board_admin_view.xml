            android:layout_below="@+id/linearLayout2">
            <ImageView
                android:id="@+id/imageView2_meeting"
                android:background="@color/white"
                android:layout_width="48dp"
                android:layout_height="48dp"
                android:src="@drawable/search"
                android:minHeight="100dp"
                android:alpha="0.7"
                android:contentDescription="TODO" />
            <TextView
                android:textSize="15sp"
                android:textColor="@color/text_grey"
                android:gravity="center_horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="No Meetings Found"/>
        </LinearLayout>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/targetList_meeting"
            android:visibility="gone"
            android:focusable="false"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"/>

        <app.zingo.mysolite.Custom.MyRegulerText
            android:id="@+id/read_meeting"
            android:layout_weight="1"
            android:layout_width="150dp"
            android:visibility="gone"
            android:layout_height="45dp"
            android:background="@drawable/button_primary_dark"
            android:layout_margin="5dp"
            android:textAlignment="center"
            android:gravity="center"
            android:focusable="false"
            android:layout_gravity="right"
            android:padding="5dp"
            android:text="Read More"
            android:textSize="22dp"
            android:textColor="@color/white"
            />





